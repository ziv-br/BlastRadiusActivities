{
    "compilerOptions": {
        "target": "ES2015",
        "noUnusedLocals": true,
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "jsx": "react",
        "sourceMap": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "allowJs": true,
    },
    "exclude": [ "dist", "*.js" ]
}